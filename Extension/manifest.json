{
  "manifest_version": 3,
  "name": "User-Agent Toggle",
  "version": "1.0",
  "permissions": ["declarativeNetRequest", "storage", "activeTab", "tabs"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
